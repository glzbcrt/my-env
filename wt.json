    "actions": [
        {
            "command": {
                "action": "newTab",
                "profile": "Ubuntu"
            },
            "keys": "f10"
        },
        {
            "command": {
                "action": "newTab"
            },
            "keys": "f9"
        },
        {
            "command": {
                "action": "commandPalette"
            },
            "keys": "f1"
        },
        {
            "command": "find",
            "keys": "f3"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "f2"
        },
        {
            "command": {
                "action": "newTab",
                "profile": "Kali Linux"
            },
            "keys": "f11"
        },
        {
            "command": "closePane",
            "keys": "f4"
        },
        {
            "command": {
                "action": "newTab",
                "profile": "${profile.name}"
            },
            "icon": "${profile.icon}",
            "iterateOn": "profiles",
            "name": "_${profile.name}"
        }
    ],
    "alwaysShowTabs": true,
    "centerOnLaunch": true,
    "copyFormatting": "none",
